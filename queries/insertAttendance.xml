<query id="insertAttendance" action="insert">
	<tables>
		<table name="attendance" />
	</tables>
	<columns>
		<column name="attendance_srl" var="attendance_srl" notnull="notnull" />
		<column name="regdate" var="regdate" default="curdate()" notnull="notnull" />
		<column name="member_srl" var="member_srl" />
		<column name="ipaddress" var="ipaddress" default="none" />
		<column name="greetings" var="greetings" default="" />
		<column name="today_point" var="today_point" />
		<column name="today_random" var="today_random" default="0" />
		<column name="att_random_set" var="att_random_set" default="0" />
		<column name="perfect_m" var="perfect_m" default="N" />
		<column name="present_y" var="present_y" default="N" />
		<column name="a_continuity" var="a_continuity" default="1" />
	</columns>
</query>
